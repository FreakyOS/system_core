[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/bunnyy (bunnyyTheFreak)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/bunnyy (bunnyyTheFreak)
	label-Verified = -1..+1 group user/bunnyy (bunnyyTheFreak)
	push = group user/bunnyy (bunnyyTheFreak)
	read = group user/bunnyy (bunnyyTheFreak)
	rebase = group user/bunnyy (bunnyyTheFreak)
	removeReviewer = group user/bunnyy (bunnyyTheFreak)
	submit = group user/bunnyy (bunnyyTheFreak)
[access "refs/heads/*"]
	create = group user/bunnyy (bunnyyTheFreak)
	push = +force group user/bunnyy (bunnyyTheFreak)
	pushMerge = group user/bunnyy (bunnyyTheFreak)
	read = group user/bunnyy (bunnyyTheFreak)
[access "refs/tags/*"]
	push = group user/bunnyy (bunnyyTheFreak)
	push = +force group user/bunnyy (bunnyyTheFreak)
